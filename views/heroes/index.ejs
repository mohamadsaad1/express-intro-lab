<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <body>
    <h1>Dota 2 Heroes Mastered?</h1>
    <% if(error) { %>
      <h2><%= error %></h2>
    <% } %>
    <ul>
      <% heroes.forEach(function(champ) { %>
        <li><%= champ.hero %> - <%= champ.mastered ? 'Definitely' : 'Never in a million years' %></li>
      <% }) %>
    </ul>
  </body>
</body>
</html>